    <!--Nishant : Added code for Dashboard Chart: Start-->
    <script>

        var pendingArray = @Html.Raw(Json.Encode(ViewBag.Completed)); //financial year 
        var completedArray = @Html.Raw(Json.Encode(ViewBag.Pending)); //expenditure amount
    var assembly_nameArray = @Html.Raw(Json.Encode(ViewBag.AssemblyName));

    const ctx = document.getElementById('myBarChart');
    new Chart(ctx, {
        type: 'bar',
        options: {
            animation: false,
            plugins: {
                legend: {
                    display: false
                },
                tooltip: {
                    enabled: false
                }
            }
        },
        data: {
            labels: pendingArray,
            datasets: [
                {
                    label: 'Pending Schemes',
                    data: pendingArray,
                    borderWidth: 1
                },
                {
                    label: 'Completed Schemes',
                    data: completedArray,
                    borderWidth: 1

                }
            ]
        },
        options: {
            scales: {
                y: {
                    beginAtZero: true
                }
            }
        }
    });

    const ctxline = document.getElementById('myLineChart');
    new Chart(ctxline, {
        type: 'line',
        options: {
            animation: false,
            plugins: {
                legend: {
                    display: false
                },
                tooltip: {
                    enabled: false
                }
            }
        },
        data: {
            labels: assembly_nameArray,
            datasets: [{
                label: 'Pending Schemes',
                data: pendingArray,
                borderWidth: 1
            },
            {
                label: 'Completed Schemes',
                data: completedArray,
                borderWidth: 1

            }
            ]
        },
        options: {
            scales: {
                y: {
                    beginAtZero: true
                }
            }
        }
    });

    const ctxpie = document.getElementById('myPieChart');
    new Chart(ctxpie, {
        type: 'pie',
        options: {
            animation: false,
            plugins: {
                legend: {
                    display: false
                },
                tooltip: {
                    enabled: false
                }
            }
        },
        data: {
            labels: assembly_nameArray,
            datasets: [{
                label: 'Pending Schemes',
                radius: '70%',
                data: pendingArray,
                borderWidth: 1
            }
            ]
        },
        options: {
            scales: {
                y: {
                    beginAtZero: true
                }
            }
        }
    });

    const ctxdoughnut = document.getElementById('myDoughnutChart');
    new Chart(ctxdoughnut, {
        type: 'doughnut',
        options: {
            animation: false,
            plugins: {
                legend: {
                    display: false
                },
                tooltip: {
                    enabled: false
                }
            }
        },
        data: {
            labels: assembly_nameArray,
            datasets: [{
                label: 'Pending Schemes',
                data: pendingArray,
                radius: '70%',
                borderWidth: 1
