            <script type="text/javascript">
                $(document).ready(function () {
                    var newid = localStorage.getItem('id');
                    var color = localStorage.getItem('current-skin');
                    if (color != '' && color != null) {
                        $('#theme').attr('data-current-skin', color);
                        // $('ul.dropdown-menu li.active').append("<span class ='glyphicon glyphicon-ok'></span>");
                        $('#' + newid).append("<i class ='glyphicon glyphicon-ok ok-icon'></i>");
                    }
                    if (newid != "" && newid != null) {

                        $('ul.dropdown-menu li').parent().find('li.active-nav').removeClass('active-nav');

                        $('#' + newid).addClass('active-nav');
                    }
                    $(function () {
                        $('ul.dropdown-menu li').on('click', function () {
                            //  alert("this.id");
                            localStorage.setItem('id', this.id);
                            var newid = localStorage.getItem('id');
                            if (newid != "" && newid != null) {

                                $('ul.dropdown-menu li').parent().find('li.active-nav i').remove();

                                $('#' + newid).append('<i class="glyphicon glyphicon-ok ok-icon"></i>');
                            }
                            $(this).parent().find('li.active-nav').removeClass('<i class="glyphicon glyphicon-ok ok-icon"></i>');

                            $(this).addClass('active-nav');
                        });
                    });
                    $('.ss-skin').click(function () {
                        $('#theme').attr('data-current-skin', $(this).data('skin'));
                        console.log($(this).data('skin'));
                        localStorage.setItem('current-skin', $(this).data('skin'));
                        alert(this.parent.id);

                        $(this.parent.id).append('<i class="glyphicon glyphicon-ok"></i>');
                        var newid = localStorage.getItem('id');
                        var color = localStorage.getItem('current-skin');
                        if (color != '' && color != null) {
                            $('#theme').attr('data-current-skin', color);
                            // $('ul.dropdown-menu li.active').append("<span class ='glyphicon glyphicon-ok'></span>");
                            $('#' + newid).append("<i class ='glyphicon glyphicon-ok ok-icon'></i>");
                        }
                    });

                });

